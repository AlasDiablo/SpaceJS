  "bugs": {
    "url": "https://github.com/AlasDiablo/SpaceJS/issues"
  },
  "homepage": "https://github.com/AlasDiablo/SpaceJS#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2"
  }
